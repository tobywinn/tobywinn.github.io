{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    
    "title": {
      "text": "Satisfaction ratings as top city firms",
      "subtitle":"Scraped from Indeed.com",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"
    },
    
     "data": {
      "url": "https://raw.githubusercontent.com/tobywinn/tobywinn.github.io/main/week6/GlassdoorRatings.csv"
    },
    
    "height": 300,
    "width": 320,
     
    "mark": {"type": "bar",
    "color": "chocolate"
    },
   
    "encoding": {
  
      "x": {
        "field": "Firm",
        "type": "nominal",
        "title": null,
        "axis": {"grid": false}
      },
  
      "y": {
        "field": "Rating",
        "type": "quantitative",
        "title": null,
        "axis": {"grid": false}
      }
  }
}